				<Item name="CALS_PREFLIGHT_CMD_PROFILE_NAME" type="text" value="拼合透明度（全部轮廓化）"/>
				<Item name="CALS_PREFLIGHT_CMD_REP_DISPLAY" type="boolean" value="false"/>
				<Item name="CALS_PREFLIGHT_CMD_SUC_ACT_TYPE" type="integer" value="0"/>
